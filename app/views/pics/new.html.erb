<div class="span6 offset3 text-center">
	<h1 class="custom-pic-header">Add a new picture</h1>
	<div class="boxy text-center">

	<%= simple_form_for @pic do |f| %> 

   		<%= f.input :hour %>
   		<%= f.input :image, :as => :file %>
   		<%= f.input :emotion %>
   		<%= f.input :learned %>
   		<%= submit_tag "Submit"%>

	<% end %>
	</div>
</div>